$(eval $(call addlib_s,UKBLAKE3,$(CONFIG_UKBLAKE3)))

CINCLUDES-$(CONFIG_UKBLAKE3)	+= -I$(UKBLAKE3_BASE)/include
CXXINCLUDES-$(CONFIG_UKBLAKE3)	+= -I$(UKBLAKE3_BASE)/include

UKBLAKE3_CFLAGS-y += -DBLAKE3_NO_AVX512 -DBLAKE3_NO_SSE2 -DBLAKE3_NO_SSE41 -DBLAKE3_NO_AVX2

UKBLAKE3_SRCS-y += $(UKBLAKE3_BASE)/blake3.c
UKBLAKE3_SRCS-y += $(UKBLAKE3_BASE)/blake3_portable.c
UKBLAKE3_SRCS-y += $(UKBLAKE3_BASE)/blake3_dispatch.c
