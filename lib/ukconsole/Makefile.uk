$(eval $(call addlib_s,libukconsole,$(CONFIG_LIBUKCONSOLE)))

CINCLUDES-$(CONFIG_LIBUKCONSOLE) += -I$(LIBUKCONSOLE_BASE)/include
CXXINCLUDES-$(CONFIG_LIBUKCONSOLE) += -I$(LIBUKCONSOLE_BASE)/include

LIBUKCONSOLE_SRCS-y += $(LIBUKCONSOLE_BASE)/console.c

ifneq ($(filter y,$(CONFIG_LIBUKCONSOLE_TEST) $(CONFIG_LIBUKTEST_ALL)),)
	LIBUKCONSOLE_SRCS-y += $(LIBUKCONSOLE_BASE)/tests/test_ukconsole.c
endif
